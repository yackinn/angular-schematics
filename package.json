{
	"name": "@yackinn/angular-schematics",
	"version": "0.0.10",
	"description": "repo for angular schematics",
	"scripts": {
		"dev": "npm run build -- -w",
		"schematic": "func() { schematics .:\"$1\" \"$2\" --\"$3\" --\"$4\"; }; func",
		"build": "tsc -p tsconfig.json",
		"test": "npm run build && jasmine src/**/*.spec.js",
    	"postbuild": "yarn run copy:files",
		"clean": "yarn run rimraf ../../dist/libs/akita/schematics",
		"copy:files": "yarn run copyfiles -u 1 \"src/**/files/**/*\" dist/"
	},
	"keywords": [
		"schematics"
	],
	"author": "",
	"license": "MIT",
	"schematics": "./src/collection.json",
	"repository": {
		"type": "git",
		"url": "https://github.com/yackinn/angular-schematics.git"
	},
	"dependencies": {
		"@angular-devkit/core": "^11.0.5",
		"@angular-devkit/schematics": "^11.0.5",
		"typescript": "~3.8.2"
	},
	"devDependencies": {
		"copyfiles": "^2.2.0",
		"@types/node": "^12.11.1",
		"@types/jasmine": "~3.5.0",
		"jasmine": "^3.5.0"
	}
}
